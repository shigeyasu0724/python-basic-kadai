        "price1 = 100  # 変数price1に100を代入（商品の価格1）\n",
        "price2 = 200  # 変数price2に200を代入（商品の価格2）\n",
        "\n",
        "tax = 1.1  # 消費税率10%（1.1倍）  <- tax を関数の外で定義\n",
        "\n",
        "def total():\n",
        "    return price1 + price2  # 商品の合計金額（税抜き）を返す\n",
        "\n",
        "print(total() * tax)  # 合計金額（税抜き）に消費税をかけて表示"
